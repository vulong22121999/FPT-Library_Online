                    <div id="Total2" class="WordSection1" style="font-weight:normal;">

                    </div>
                    <script>
                    document.getElementById('Total2').innerHTML = '<span> Bằng chữ: ' + jsUcfirst(DOCSO.doc(@SUM2)) + '</span>';
