                                <td>@item.NgayChungTu</td>
                                <td>@item.POID</td>
                                <td>@item.DKCB</td>
                                <td>@item.ISBN</td>
                                <td>@item.NhanDe</td>
                                <td>@item.NhaXuatBan</td>
                                <td>@item.NamXuatBan</td>
                                <td>@item.SLN</td>
                                <td>@item.NgayBoSung</td>
                                <td>@item.DonGia</td>
                                <td>@item.DonViTienTe</td>
                                <td>@item.ThanhTien</td>
                                <td>@item.UseCount</td>
                            </tr>
                            SUM3 += item.ThanhTien;
                            TOTAL3 += item.SLN;
                        }
                        <tr>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>Tổng cộng</td>
                            <td></td>
                            <td></td>
                            <td>@TOTAL3</td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>@SUM3</td>
                            <td></td>
                        </tr>
                    </table>
                    <div id="Total3" class="WordSection1" style="font-weight:normal;">

                    </div>
                    <script>
                    document.getElementById('Total3').innerHTML = '<span> Bằng chữ: ' + jsUcfirst(DOCSO.doc(@SUM3)) + '</span>';
                    </script>
                }
            }
        }


        <table class="WordSection1" style="margin-top:20px; text-align:center;" width="100%" border="0">
